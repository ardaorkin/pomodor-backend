{
  "name": "pomodoro-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./server/bin/www --exec babel-node -e js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/app.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.5",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^8.2.5"
  }
}
